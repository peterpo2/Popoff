{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": ""
  },
  "AppUrls": {
    "AllowedOrigins": "http://localhost:4173"
  },
  "CoreServers": [
    {
      "Name": "Local-Docker-Host",
      "Ip": "127.0.0.1",
      "IsPrimary": true
    }
  ],
  "Docker": {
    "Enabled": false,
    "SocketPath": "/var/run/docker.sock"
  },
  "ExternalServers": {
    "Servers": [
      {
        "Name": "Remote-Dev-Agent",
        "BaseUrl": "https://agent.dev.example.com",
        "ApiKey": "dev-api-key",
        "Type": "RemoteAgent"
      }
    ]
  },
  "Jwt": {
    "Issuer": "crm.popoff.com",
    "Audience": "crm.popoff.com",
    "Key": "",
    "ExpiryMinutes": 120
  },
  "HealthCheck": {
    "IntervalSeconds": 60,
    "TimeoutSeconds": 10,
    "AppendHealthPath": true,
    "HealthPath": "/health"
  }
}
